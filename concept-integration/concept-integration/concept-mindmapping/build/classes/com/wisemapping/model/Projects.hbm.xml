<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.wisemapping.model.Projects" table="PROJECTS">
		<id name="id">
			<generator class="increment" />
		</id>
		<property name="title" unique="false" not-null="true" />
		<property name="description" unique="false" not-null="false" />
	</class>
</hibernate-mapping>